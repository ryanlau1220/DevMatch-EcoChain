{
  "name": "ecochain-subgraph",
  "version": "1.0.0",
  "description": "EcoChain Environmental Data Subgraph with Token API Integration",
  "main": "index.js",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "graph deploy --node https://api.studio.thegraph.com/deploy/",
